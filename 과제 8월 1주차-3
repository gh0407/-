#include <stdio.h>
int main()
{
	int num1;
	int num2;
	printf("두 개의 숫자를 입력하세요 >");
	scanf("%d %d", &num1, &num2);
	if(num1 == num2) printf("두 수는 같다.\n");
	if(num1 != num2) printf("두 수는 같지 않다.\n");
	if(num1 > num2) printf("num1이 num2보다 크다.\n");
    if(num1 < num2) printf("num2가  num1보다 크다.\n");
return 0;
}
